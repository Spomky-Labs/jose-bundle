{
    "name": "spomky-labs/jose-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Jose Bundle",
    "keywords": ["JWS", "JWT", "JWE", "JWA", "JWK", "JWKSet", "Jose", "Jot", "Bundle", "Symfony"],
    "homepage": "https://github.com/Spomky-Labs/jose-bundle",
    "license": "MIT",
    "authors": [
        {
          "name": "Florent Morselli"
        }
    ],
    "require": {
        "spomky-labs/jose": "^6.0@dev",
        "symfony/console": "^2.7|^3.0",
        "symfony/http-kernel": "^2.7|^3.0",
        "symfony/framework-bundle": "^2.7|^3.0",
        "symfony/dependency-injection": "^2.7|^3.0",
        "symfony/config": "^2.7|^3.0"
    },
    "require-dev": {
        "symfony/finder": "^2.7|^3.0",
        "symfony/var-dumper": "^2.7|^3.0",
        "behat/behat": "^3.1",
        "behat/mink": "^1.5",
        "behat/mink-extension": "^2.0",
        "behat/mink-browserkit-driver":  "^1.1",
        "behat/symfony2-extension":  "^2.1",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "SpomkyLabs\\JoseBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpomkyLabs\\TestJoseBundle\\": "Tests/TestJoseBundle",
            "SpomkyLabs\\JoseBundle\\Features\\Context\\": "Tests/Context"
        }
    },
    "scripts":{
        "test": "behat"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
